{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch UI Framework",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/main",  // Path to your executable
      "args": [],                                    // Add any command-line args here
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
